{
    "name":"RMA",
    "version":"1.300",
    "url":null,
    "custom_title":"无法无天的瑠実_威力加强版",
    "default_controller": "adb",
    "lock_controller":false,
    "controller":[
        {
            "name":"ADB 默认方式",
            "type":"Adb"
        },
        {
            "name":"Win32",
            "type":"Win32",
            "input":1,
            "screencap":4
        }
    ],
    "resource": [
        {
            "name": "B 服",
            "path": [
                "{PROJECT_DIR}/Resource"
            ]
        }
    ],
    "task": [
        {
            "name":"代理作战",
            "entry":"进入战斗",
            "advanced": ["作业名称"]
        },
        {
            "name":"代理刷本",
            "entry":"周回副本_选择票券",
            "advanced": ["作业名称"],
            "option":["重复刷取"]
        },
        {
            "name":"技术模拟战刷奖励",
            "entry":"技术模拟战_刷分开始",
            "option":[
                "刷分关卡"
            ]
        },
        {
            "name":"速度测试",
            "entry":"111"
        }
    ],
    "advanced": {
        "作业名称":{
            "field":"作业名称",
            "type":"string",
            "default":"请输入作业名称",
            "pipeline_override":{
                "进入战斗":{
                    "next":["{作业名称}_1轮_进入回合"]
                }
            }
        }
    },
    "option": {
        "重复刷取":{
            "cases":[
                {
                    "name":"不重复刷"
                },
                {
                    "name":"要重复刷",
                    "pipeline_override":{
                        "胜利轮进入回合":{
                            "next":["周回副本_战斗奖励"]
                        }
                    }
                }
            ]
        },
        "异时层强敌":{
            "cases":[
                {
                    "name":"死亡蛞蝓",
                    "pipeline_override":{
                        "异时层强敌_选择敌人":{
                            "expected":"死亡"
                        }
                    }
                },
                {
                    "name":"畸旋钻",
                    "pipeline_override":{
                        "异时层强敌_选择敌人":{
                            "expected":"畸旋钻"
                        }
                    }
                },
                {
                    "name":"猩红之主",
                    "pipeline_override":{
                        "异时层强敌_选择敌人":{
                            "expected":"红之主"
                        }
                    }
                },
                {
                    "name":"感应触须",
                    "pipeline_override":{
                        "异时层强敌_选择敌人":{
                            "expected":"感应"
                        }
                    }
                },
                {
                    "name":"普通覆地之手",
                    "pipeline_override":{
                        "异时层强敌_选择敌人":{
                            "expected":"第三章Day21"
                        }
                    }
                },
                {
                    "name":"终极触须",
                    "pipeline_override":{
                        "异时层强敌_选择敌人":{
                            "expected":"终极"
                        }
                    }
                },
                {
                    "name":"究极覆地之手",
                    "pipeline_override":{
                        "异时层强敌_选择敌人":{
                            "expected":"上篇Day12"
                        }
                    }
                },
                {
                    "name":"沙漠鞭虫",
                    "pipeline_override":{
                        "异时层强敌_选择敌人":{
                            "expected":"沙漠鞭虫"
                        }
                    }
                },
                {
                    "name":"亡骨之翎第一形态",
                    "pipeline_override":{
                        "异时层强敌_选择敌人":{
                            "expected":"第一形态"
                        }
                    }
                }
            ]
        },
        "练习模式":{
            "cases":[
                {
                    "name":"开启练习"
                },
                {
                    "name":"用票挑战",
                    "pipeline_override":{
                        "异时层_勾选练习":{
                            "template": "异时层_挑战.png"
                        }
                    }
                }
            ]
        },
        "使用票数":{
            "cases":[
                {
                    "name":"1",
                    "pipeline_override":{
                        "异时层强敌_点击ok_异时层票券数量":{
                            "expected":"1"
                        }
                    }
                },
                {
                    "name":"2",
                    "pipeline_override":{
                        "异时层强敌_点击ok_异时层票券数量":{
                            "expected":"2"
                        }
                    }
                },
                {
                    "name":"3",
                    "pipeline_override":{
                        "异时层强敌_点击ok_异时层票券数量":{
                            "expected":"3"
                        }
                    }
                },
                {
                    "name":"4",
                    "pipeline_override":{
                        "异时层强敌_点击ok_异时层票券数量":{
                            "expected":"4"
                        }
                    }
                },
                {
                    "name":"5",
                    "pipeline_override":{
                        "异时层强敌_点击ok_异时层票券数量":{
                            "expected":"5"
                        }
                    }
                }
            ]
        },
        "刷分关卡":{
            "cases":[
                {
                    "name":"31A普通"
                },
                {
                    "name":"31C普通",
                    "pipeline_override":{
                        "技术模拟战_选择31A普通":{
                            "template":"技术模拟战_31C普通.png"
                        }
                    }
                }
            ]
        },
    }
}